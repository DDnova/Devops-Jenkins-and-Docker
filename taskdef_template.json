{
  "family": "%TASK_DEFINITION_NAME%",
  "networkMode": "awsvpc",
  "executionRoleArn": "%EXECUTION_ROLE_ARN%",
  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URI%:%SHORT_COMMIT%",
      "name": "%SERVICE_NAME%",
      "cpu": 2,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
